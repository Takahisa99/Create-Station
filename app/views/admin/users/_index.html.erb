      <table class='table table-hover table-inverse'>
        <thead>
          <tr>
            <th>会員ID</th>
            <th>会員名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% users.each do |user| %>
           <tr>
             <td><%= user.id %></td>
             <td><%= link_to admin_user_path(user.id) do %>
                 <%= user.name %>
                 <% end %>
             </td>
             <td><%= user.email  %></td>
              <%# @total_order_items += order_detail.quantity %>
             <td>
               <% if user.is_deleted == true %>
                   <span class="text-secondary" ><strong>退会</strong></span>
               <% else %>
               　  <span class="text-success" ><strong>有効</strong></span>
               <% end %>
              </td>
            </tr>
            <tr>

            </tr>
          <% end %>

        </tbody>
      </table>